
<h2><%= @target_message %></h2>
<br>
<h3>
  <%= @target.first_name %> <%= @target.last_name %> has  <%= pluralize(@action_array[0][:senator_count], 'senator') %>
</h3>
<h3>
  <% @action_array[1,@action_array.length].each do |r| %>

      <%if r[:rep_type] == "S" %>
        <h3>  <%= r[:rep_name]%> </h3>
      <% end %>
  <% end %>
</h3>
