<div class="message-edit">
    <div class="col-sm-6 col-sm-offset-3 col-xs-10 col-xs-offset-1">
        <div>
            <h3>Message Distribution History</h3>
        </div>

        <div class="row top-spacer">
            <% @history = Messhistory.where(message_id: @message.id)%>
            <% @history.each do |h| %>
            <div class="row index-list">
                <%= "Sent" + ' ' + h.sent_date.to_s%>
            </div>
            <% end %>
        </div>
    </div>
</div>
